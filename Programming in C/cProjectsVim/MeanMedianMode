#include <stdio.h>

void main()
{
	int arraySize = 10;
	int* arr = malloc(sizeof(int)*arraySize);
	for(int i = 0; i<arraySize; i++)
		{
		*(arr+i) = rand();
		}
	for(int i = 0; i < arraySize; i++)
		{
		int allTogether += *(arr+i);
		if(i == 9)
			{
			double allTogetherD = allTogether / arraySize;
			printf("Mean is : %f\n",allTogetherD);
			} 
		}
